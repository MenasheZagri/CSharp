// 12345 Pyramid

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp9
{
    class Program
    {
        static void Main(string[] args)
        {
            for (int i = 1; i <= 6; i=i+1)
            {
                for (int j = 1; j < i; j=j+1)
                {
                Console.Write(j);
                }
                Console.WriteLine();
                                
                    
                    
               
            }
        }
    }
}
