//2 same ways of calling elements from array using a forech loop:

            List<int> numbers = new List<int>();
            numbers.Add(1);
            numbers.Add(5);

//1- var 
            foreach (var VARIABLE in numbers)
            {
                Console.WriteLine(VARIABLE);
            }

//2- type specific

            foreach (int item in numbers)
            {
                Console.WriteLine(item);
            }
