//Asterist inversed pyramid

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp9
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = Convert.ToInt32(Console.ReadLine());
            Console.Clear();
            for (int i = n; i > 0; i=i-1)
            {
                for (int j = 1; j <= i; j++)
                {
                    Console.Write("*");
                    
                }
                Console.WriteLine("");
            
              
                
            }

            

        }
    }
}
