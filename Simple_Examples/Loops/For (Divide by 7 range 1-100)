
// Divide by 7 range 1-100
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp14
{
    class Program
    {
        static void Main(string[] args)
        {
            for (int i = 7; i < 100; i=i+7)
            {
                Console.WriteLine(i);
            }
        }
    }
}
