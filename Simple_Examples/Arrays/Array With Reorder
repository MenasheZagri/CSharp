using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp15
{
    class Program
    {
        static void Main(string[] args)
        {

            Console.WriteLine("Enter Ages of family members");


            int[] arr = new int[5];
            arr[0] = Convert.ToInt32(Console.ReadLine());
            arr[1] = Convert.ToInt32(Console.ReadLine());
            arr[2] = Convert.ToInt32(Console.ReadLine());
            arr[3] = Convert.ToInt32(Console.ReadLine());
            arr[4] = Convert.ToInt32(Console.ReadLine());

            Array.Sort(arr);

            Array.Reverse(arr);

            Console.WriteLine("");
            Console.WriteLine("The ages from top to buttom:\n");


            foreach (int value in arr)
            {
                Console.WriteLine(value + " ");


            }
        }
    }
}
